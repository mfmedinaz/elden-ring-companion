{
  "expo": {
    "name": "Elden Ring Companion",
    "slug": "elden-ring",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/torrent-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/bandai-namco.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#050D06"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/torrent-icon.png",
        "backgroundColor": "#050D06"
      },
      "package": "com.medzam.eldenring"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/torrent-icon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "da2a8390-39ec-4899-887e-e924a559f5b6"
      }
    }
  }
}
